"color vilight
color base16-woodsy
"set guifont=Menlo\ Regular:h14
"set guifont=Consolas\ Bold:h15
set linespace=2
set antialias

" Don't beep
set visualbell

" Start without the toolbar
set guioptions-=T

if has("gui_macvim")
  " Fullscreen takes up entire screen
  set fuoptions=maxhorz,maxvert
end
